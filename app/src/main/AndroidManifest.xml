<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="studienarbeit.beta02_notfallapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <uses-feature
            android:name="android.hardware.camera"
            android:required="true" />

        <activity android:name=".GPS" />
        <activity android:name=".Anruf" />
        <activity android:name=".Sprachnachricht" />
        <activity android:name=".Heimroute" />
        <activity android:name=".Einstellungen" />
        <activity android:name=".Bildaufnahme"></activity>
    </application>

</manifest>
